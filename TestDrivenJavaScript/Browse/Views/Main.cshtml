<!-- ko with: browse -->
<div class="panel panel-default">
    <!-- Default panel contents -->
    <div class="panel-heading">To Do items</div>

    <!-- Table -->
    <!-- ko with: toDoList -->
    <table class="table">
        <thead>
            <tr>
                <th>#</th>
                <th>Title</th>
                <th>Priority</th>
                <th>Created on</th>
            </tr>
        </thead>
        <tbody data-bind="foreach: items">
            <tr>
                <td data-bind="text: title"></td>
                <td data-bind="text: priority"></td>
                <td data-bind="text: createdOn"></td>
            </tr>
        </tbody>
    </table>
    <!-- /ko -->
</div>
<!-- /ko -->